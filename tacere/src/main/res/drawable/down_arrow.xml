<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="-40%"
            android:pivotY="87%"
            android:toDegrees="45">
            <shape
                android:shape="rectangle">
                <stroke
                    android:width="10dp"
                    android:color="#00ffffff"/>
                <solid
                    android:color="@color/primary"/>
            </shape>
        </rotate>
    </item>
</layer-list>